{
    "name": "condor-submit-file",
    "displayName": "HTCondor Submit File",
    "description": "Syntax Highlighting for HTCondor's submit_file format.",
    "repository": {
        "type": "git",
        "url": "https://github.com/evangelos-ch/vscode-condor-submit-file.git"
    },
    "icon": "htcondor.png",
    "galleryBanner.color": "#eb4d4d",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "submit_file",
                "aliases": [
                    "HTCondor submit_file",
                    "submit_file"
                ],
                "extensions": [
                    ".submit_file"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "submit_file",
                "scopeName": "source.submit_file",
                "path": "./syntaxes/submit_file.tmLanguage.json"
            }
        ]
    }
}
